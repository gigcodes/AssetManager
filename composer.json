{
    "name": "gigcodes/asset-manager",
    "description": "Asset Manager Laravel package for Gigcodes UI Library",
    "type": "library",
    "license": "MIT",
    "keywords": ["gigcodes", "media manager", "uploader", "laravel"],
    "autoload": {
        "files": [
          "src/helpers.php"
        ],
        "psr-4": {
            "Gigcodes\\AssetManager\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Karthick",
            "email": "karthickk1996@hotmail.com"
        }
    ],
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "league/flysystem": "^2.4",
        "intervention/image": "^2.7",
        "maennchen/zipstream-php": "^2.1"
    },
    "extra": {
        "laravel": {
            "providers" : [
                "Gigcodes\\AssetManager\\AssetManagerServiceProvider"
            ]
        }
    }
}
